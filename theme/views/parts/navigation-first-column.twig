<ul class="nav-main">
  {% set count_first = 1 %}

  {% for item in first_column.items %}



    <li
      class="header-dropdown-item nav-main-item {{ item.children ? 'children': '' }} {{ item.classes|join(' ') }}">

      <a data-current="{{ first_column.title|replace({' ': ''})|lower }}" data-open="{{ item.slug }}"
         class="nav-main-link"
         href="{{ item.link }}" {% if item._menu_item_target %}target="{{ item._menu_item_target }}"{% endif %}>{{ item.title }}</a>

    </li>
    {% set count_first = count_first + 1 %}
  {% endfor %}
</ul>
